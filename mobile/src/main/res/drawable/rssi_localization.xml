<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/welcome_area">
        <shape android:shape="rectangle">
            <solid android:color="#FF000000" />
            <stroke android:width="1dp" android:color="#FF000000" />
        </shape>
    </item>
    <item android:id="@+id/lock_area" android:top="10dp" android:right="10dp"
        android:bottom="10dp" android:left="10dp">
        <shape android:shape="rectangle">
            <solid android:color="#FF000000" />
            <stroke android:width="1dp" android:color="#FF000000" />
        </shape>
    </item>
    <!-- start -->
    <item android:id="@+id/start_area" android:top="40dp" android:right="60dp"
        android:bottom="40dp" android:left="90dp">
        <shape android:shape="rectangle">
            <solid android:color="#FF000000" />
            <stroke android:width="1dp" android:color="#FF000000" />
        </shape>
    </item>
    <item android:id="@+id/car" android:top="10dp" android:right="10dp"
        android:bottom="10dp" android:left="10dp"
        android:gravity="center"
        android:drawable="@drawable/rssi_localization_car"/>
    <!-- left entry -->
    <item android:id="@+id/unlock_area_left" android:top="110dp" android:right="60dp"
        android:bottom="20dp" android:left="80dp">
        <shape android:shape="rectangle">
            <solid android:color="#FF000000" />
            <stroke android:width="1dp" android:color="#FF000000" />
        </shape>
    </item>
    <!-- right entry -->
    <item android:id="@+id/unlock_area_right" android:top="20dp" android:right="60dp"
        android:bottom="110dp" android:left="80dp">
        <shape android:shape="rectangle">
            <solid android:color="#FF000000" />
            <stroke android:width="1dp" android:color="#FF000000" />
        </shape>
    </item>
    <!-- back entry -->
    <item android:id="@+id/unlock_area_back" android:top="40dp" android:right="30dp"
        android:bottom="40dp" android:left="210dp">
        <shape android:shape="rectangle">
            <solid android:color="#FF000000" />
            <stroke android:width="1dp" android:color="#FF000000" />
        </shape>
    </item>
</layer-list>