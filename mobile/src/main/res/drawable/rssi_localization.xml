<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/welcome_area">
        <shape android:shape="rectangle">
            <solid android:color="#FF000000" />
            <stroke
                android:width="1dp"
                android:color="#FF000000" />
        </shape>
    </item>
    <item
        android:id="@+id/lock_area"
        android:bottom="10dp"
        android:left="10dp"
        android:right="10dp"
        android:top="10dp">
        <shape android:shape="rectangle">
            <solid android:color="#FF000000" />
            <stroke
                android:width="1dp"
                android:color="#FF000000" />
        </shape>
    </item>
    <!-- start -->
    <item
        android:id="@+id/start_area_fl"
        android:bottom="40dp"
        android:left="80dp"
        android:right="120dp"
        android:top="70dp">
        <shape android:shape="rectangle">
            <solid android:color="#FF000000" />
        </shape>
    </item>
    <item
        android:id="@+id/start_area_fr"
        android:bottom="75dp"
        android:left="80dp"
        android:right="120dp"
        android:top="40dp">
        <shape android:shape="rectangle">
            <solid android:color="#FF000000" />
        </shape>
    </item>
    <item
        android:id="@+id/start_area_rl"
        android:bottom="40dp"
        android:left="150dp"
        android:right="80dp"
        android:top="70dp">
        <shape android:shape="rectangle">
            <solid android:color="#FF000000" />
        </shape>
    </item>
    <item
        android:id="@+id/start_area_rr"
        android:bottom="70dp"
        android:left="150dp"
        android:right="80dp"
        android:top="40dp">
        <shape android:shape="rectangle">
            <solid android:color="#FF000000" />
        </shape>
    </item>
    <!-- trunk -->
    <item
        android:id="@+id/trunk_area"
        android:bottom="40dp"
        android:left="190dp"
        android:right="64dp"
        android:top="40dp">
        <shape android:shape="rectangle">
            <solid android:color="#FF000000" />
            <stroke
                android:width="1dp"
                android:color="#FF000000" />
        </shape>
    </item>
    <!-- left entry -->
    <item
        android:id="@+id/unlock_area_left"
        android:bottom="20dp"
        android:left="100dp"
        android:right="80dp"
        android:top="110dp">
        <shape android:shape="rectangle">
            <solid android:color="#FF000000" />
            <stroke
                android:width="1dp"
                android:color="#FF000000" />
        </shape>
    </item>
    <!-- right entry -->
    <item
        android:id="@+id/unlock_area_right"
        android:bottom="110dp"
        android:left="100dp"
        android:right="80dp"
        android:top="20dp">
        <shape android:shape="rectangle">
            <solid android:color="#FF000000" />
            <stroke
                android:width="1dp"
                android:color="#FF000000" />
        </shape>
    </item>
    <!-- front entry -->
    <item
        android:id="@+id/unlock_area_front"
        android:bottom="40dp"
        android:left="30dp"
        android:right="216dp"
        android:top="40dp">
        <shape android:shape="rectangle">
            <solid android:color="#FF000000" />
            <stroke
                android:width="1dp"
                android:color="#FF000000" />
        </shape>
    </item>
    <!-- back entry -->
    <item
        android:id="@+id/unlock_area_back"
        android:bottom="40dp"
        android:left="216dp"
        android:right="30dp"
        android:top="40dp">
        <shape android:shape="rectangle">
            <solid android:color="#FF000000" />
            <stroke
                android:width="1dp"
                android:color="#FF000000" />
        </shape>
    </item>
    <!-- thatcham area -->
    <item
        android:id="@+id/thatcham_area"
        android:bottom="120dp"
        android:left="10dp"
        android:right="240dp"
        android:top="10dp">
        <shape android:shape="rectangle">
            <solid android:color="#FF000000" />
            <stroke
                android:width="1dp"
                android:color="#FF000000" />
        </shape>
    </item>
    <!-- remote parking area -->
    <item
        android:id="@+id/remote_parking_area"
        android:bottom="120dp"
        android:left="240dp"
        android:right="10dp"
        android:top="10dp">
        <shape android:shape="rectangle">
            <solid android:color="#FF000000" />
            <stroke
                android:width="1dp"
                android:color="#FF000000" />
        </shape>
    </item>
    <item
        android:id="@+id/car_drawable"
        android:bottom="10dp"
        android:drawable="@drawable/car_8"
        android:gravity="center"
        android:left="10dp"
        android:right="10dp"
        android:top="10dp" />
    <!-- rooftop -->
    <item
        android:id="@+id/rooftop"
        android:bottom="45dp"
        android:left="90dp"
        android:right="70dp"
        android:top="45dp">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/transparent" />
            <stroke
                android:width="7dp"
                android:color="@android:color/transparent" />
        </shape>
    </item>
</layer-list>